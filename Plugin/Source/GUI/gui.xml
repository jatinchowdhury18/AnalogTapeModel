<?xml version="1.0" encoding="UTF-8"?>

<magic>
  <Styles>
    <Style name="default">
      <Nodes/>
      <Classes>
        <plot-view border="2" background-color="black" border-color="silver" display="contents"/>
        <nomargin margin="0" padding="0" border="0"/>
        <group margin="5" padding="5" border="2" flex-direction="column"/>
        <Slider background-color="00000000" caption-color="FFFFFFFF" slider-text-outline="00000000"
                slider-type="rotary-horizontal-vertical" slider-textbox="textbox-below">
          <media/>
        </Slider>
      </Classes>
      <Types>
        <Slider border="0" slider-type="rotary-horizontal-vertical" slider-textbox="textbox-below"/>
        <ToggleButton border="0" max-height="50" caption-size="0" text="Active"/>
        <TextButton border="0" max-height="50" caption-size="0"/>
        <ComboBox border="0" max-height="50" caption-size="0"/>
        <Plot border="0" margin="0" padding="0" background-color="00000000"
              radius="0"/>
        <XYDragComponent border="0" margin="0" padding="0" background-color="00000000"
                         radius="0"/>
      </Types>
    </Style>
  </Styles>
  <View id="root" resizable="1" resize-corner="1" flex-direction="column"
        padding="0" width="600" height="650" background-color="FF8B3232"
        background-image="Background_svg" image-placement="stretch">
    <View max-height="100" padding="0" margin="0" background-color="">
      <View margin="2" padding="" background-color="00000000" flex-direction="column"
            flex-grow="0.75">
        <TitleComp background-color="00000000" title="Chow Tape Model" font="40"
                   padding="0"/>
        <InfoComp background-color="00000000" text1="FFEAA92C" flex-grow="0.7"
                  padding="0" margin="5"/>
      </View>
      <Plot source="scope" plot-fill-color="33000000" plot-color="FFEAA92C"
            padding="0" background-color="33000000" plot-decay="0.0"/>
    </View>
    <View padding="0" margin="" background-color="">
      <View flex-direction="column" margin="5" padding="" background-color="FF31323A">
        <Label max-height="5" background-color="FF31323A"/>
        <Slider caption="Input Gain [dB]" parameter="ingain" max-height="160"
                class="Slider" tooltip="Sets the input gain to the tape model in  Decibels."
                name="Input Gain"/>
        <ComboBox caption="Oversampling" parameter="os" max-height="70" class="Slider"
                  caption-size="20"/>
        <Slider caption="Dry/Wet" parameter="drywet" max-height="170" class="Slider"/>
        <Slider caption="Output Gain [dB]" parameter="outgain" max-height="170"
                class="Slider"/>
      </View>
      <View flex-direction="column" tab-color="" background-color="FF31323A">
        <Label max-height="40" text="Hysteresis" font-size="18" justification="centred"
               background-color="00000000"/>
        <Slider caption="Bias" parameter="width" class="Slider"/>
        <Slider caption="Saturation" parameter="sat" class="Slider"/>
        <Slider caption="Drive" parameter="drive" class="Slider"/>
      </View>
      <View display="tabbed" padding="0" background-color="FF31323A">
        <View flex-direction="column" tab-caption="Loss" tab-color="" background-color="FF31323A"
              padding="0" margin="0">
          <Label max-height="40" text="Loss" justification="centred" font-size="18"
                 padding="0" margin="0" background-color="00000000"/>
          <Slider caption="Gap [mm]" parameter="gap" slider-type="linear-horizontal"
                  class="Slider" padding="0" slider-background="ff596b6c" slider-track="ffce5757"/>
          <Slider caption="Thickness [mm]" parameter="thick" class="Slider" slider-type="linear-horizontal"
                  padding="0" slider-background="ff596b6c" slider-track="ffce5757"/>
          <Slider caption="Spacing [mm]" parameter="spacing" slider-type="linear-horizontal"
                  class="Slider" padding="0" slider-background="ff596b6c" slider-track="ffce5757"/>
          <Slider caption="Speed [ips]" parameter="speed" slider-type="linear-horizontal"
                  class="Slider" padding="0" slider-background="ff596b6c" slider-track="ffce5757"/>
        </View>
        <View tab-caption="Degr." padding="0" flex-direction="column" background-color="FF31323A">
          <Label text="Degrade" justification="centred" font-size="18" max-height="40"
                 background-color="00000000"/>
          <Slider parameter="deg_depth" caption="Depth" class="Slider"/>
          <Slider caption="Amount" parameter="deg_amt" class="Slider"/>
          <Slider parameter="deg_var" caption="Variance" class="Slider"/>
        </View>
        <View tab-caption="CHEW" padding="0" flex-direction="column" background-color="FF31323A">
          <Label text="CHEW" justification="centred" font-size="18" max-height="40"
                 background-color="00000000"/>
          <Slider parameter="chew_depth" caption="Depth" max-height="180" class="Slider"/>
          <Slider caption="Frequency" parameter="chew_freq" max-height="200" class="Slider"/>
        </View>
      </View>
      <View display="tabbed" padding="0" background-color="FF31323A">
        <View tab-caption="Flutter" flex-direction="column" background-color="FF31323A">
          <Label max-height="40" text="Flutter" justification="centred" font-size="18"
                 background-color="00000000"/>
          <Slider caption="Depth" parameter="depth" max-height="200" class="Slider"/>
          <Slider caption="Rate" parameter="rate" class="Slider" max-height="200"/>
        </View>
        <View tab-caption="Wow" flex-direction="column" background-color="FF31323A">
          <Label max-height="40" text="Wow" justification="centred" font-size="18"
                 background-color="00000000"/>
          <Slider caption="Depth" parameter="wow_depth" slider-type="rotary-horizontal-vertical"
                  slider-textbox="textbox-below" max-height="200" class="Slider"/>
          <Slider caption="Rate" parameter="wow_rate" class="Slider" max-height="200"/>
        </View>
      </View>
    </View>
    <TooltipComp flex-grow="0.12" background-color="00000000" tooltip-name="FFEAA92C"/>
    <presets max-height="30" margin="0" padding="0"/>
  </View>
</magic>
